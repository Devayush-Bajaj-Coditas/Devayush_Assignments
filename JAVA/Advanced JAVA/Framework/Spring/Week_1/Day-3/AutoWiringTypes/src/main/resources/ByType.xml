<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean  class="com.bean.Address" id ="address">
        <property name="houseName" value="15"/>
        <property name="city" value="bhopal"/>
        <property name="state" value="MP"/>

    </bean>

    <bean class = "com.bean.Employee" id="Employee"  autowire="byType"> <!--   AUTOWIRE -->
        <property name ="eid" value="15"/>
        <property name="name" value ="Dev"/>
        <!--<property name = "address" ref ="address" />-->
    </bean>


</beans>